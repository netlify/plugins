{
  "name": "@netlify/plugins-list",
  "version": "2.21.0",
  "description": "List of Netlify plugins",
  "main": "site/plugins.json",
  "files": [],
  "scripts": {
    "prepublishOnly": "npm ci && npm run lint && npm run format:ci && npm test",
    "lint": "eslint 'functions/**/*.js'",
    "format": "prettier --write 'functions/**/*.js'",
    "format:ci": "prettier --check 'functions/**/*.js'",
    "test": "ava --node-arguments=--experimental-json-modules"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "netlify-plugin",
    "netlify"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^13.0.0",
    "@commitlint/config-conventional": "^13.0.0",
    "ava": "^3.13.0",
    "babel-eslint": "^10.1.0",
    "diff": "^5.0.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "got": "^11.8.0",
    "husky": "^4.2.5",
    "is-plain-obj": "^4.0.0",
    "node-fetch": "^2.6.1",
    "normalize-node-version": "^11.0.0",
    "pacote": "^11.1.13",
    "prettier": "^2.0.5",
    "semver": "^7.0.0",
    "upper-case-first": "^2.0.2"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "ava": {
    "timeout": "2m",
    "verbose": true
  }
}
